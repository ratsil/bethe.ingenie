<?xml version="1.0" encoding="utf-8" ?>
<preferences>
	<reload>
		<attribute>logger.level</attribute>
		<node>logger.excludes</node>
	</reload>
	<btl cuda="true">
		<device target="0" type="output" fps="25">
			<audio rate="48000" channels="8" bits="16" volume_change="0">true</audio>
			<video format="pal" pixels="bgra" key="false">true</video>
			<queue device="5" btl="25">
				<bias slip="7" delay="200">1</bias>
			</queue>
		</device>
	</btl>
	<ingenie>
		<userspace server="127.0.0.1" />
		<shared port="1238" />
	</ingenie>
	<helpers>
		<pixelsmap>
			<cuda version="21" />
		</pixelsmap>
		<common>
			<logger level="debug1">
				<mail subject="CHANNEL" source="alerting@example.com" server="smtp.example.com" password="">
					<targets>
						<target level="warning">user1@example.com,user2@example.com</target>
						<target level="error">user@example.com</target>
						<target level="fatal">user@example.com</target>
					</targets>
				</mail>
				<excludes>
					<pattern>*abnormal*</pattern>
				</excludes>
			</logger>
		</common>
	</helpers>
</preferences>